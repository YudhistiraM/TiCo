      { y: '2014', a: 50, b: 40, c: 65 },
      { y: '2015', a: 75, b: 65, c: 65 },
      { y: '2016', a: 50, b: 40, c: 65 },
      { y: '2017', a: 75, b: 65, c: 65 },
      { y: '2018', a: 90, b: 90, c: 65 }
    ],
    xkey: 'y',
    ykeys: ['a', 'b' , 'c'],
    labels: ['Series A', 'Series B', 'Series C']
  });
